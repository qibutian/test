        <activity android:name=".my.MyReservationsListActivity" >
        </activity>
        <activity android:name=".my.ReviseEmailActivity" >
        </activity>
        <activity android:name=".my.RevisePswdActivity" >
        </activity>
        <activity android:name=".main.GuidanceActivity" >
        </activity>
        <activity android:name="com.means.foods.main.ReadyActivity" >
        </activity>
        
         <activity
            android:name=".wxapi.WXPayEntryActivity"
            android:exported="true"
            android:launchMode="singleTop"/>

        <receiver android:name=".AppRegister" >
            <intent-filter>
                <action android:name="com.tencent.mm.plugin.openapi.Intent.ACTION_REFRESH_WXAPP" />
            </intent-filter>
        </receiver>
    </application>

</manifest>