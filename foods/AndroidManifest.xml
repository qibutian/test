        <activity android:name=".my.MsgDetailActivity" />
        <activity android:name=".cate.ReservationsDetailsActivity" />
        <activity
            android:name=".my.RegisterOneActivity"
            android:launchMode="singleTask" />
        <activity android:name=".my.RegisterTwoActivity" />
        <activity android:name=".my.RegisterThreeActivity" />
        <activity android:name=".main.MainActivity" />
        <activity
            android:name=".my.LoginActivity"
            android:launchMode="singleTask" />
        <activity android:name=".main.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".cate.ConfirmDetailsActivity" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="wx63ff0f643d107913" />
            </intent-filter>
        </activity>
        <activity android:name=".my.ReviseNameActivity" >
